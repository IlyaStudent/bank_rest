# Скопируйте этот файл в secrets.yml и заполните реальными значениями
# cp secrets.yml.example secrets.yml
# Для кодирования: echo -n 'значение' | base64
apiVersion: v1
kind: Secret
metadata:
  name: bank-rest-secrets
  namespace: bank-rest
type: Opaque
data:
  DATABASE_PASSWORD: <base64-encoded-password>
  ENCRYPTION_KEY: <base64-encoded-key-min-32-chars>
  JWT_SECRET: <base64-encoded-jwt-secret>